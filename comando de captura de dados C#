    .cid-r3yZJLuMgC{
        background-image:  url('provaup.com.br/sistema/public/img') !important;
        background-size: contain;
    }
@-webkit-keyframes spin {
  0% { -webkit-transform: rotate(0deg); }
  100% { -webkit-transform: rotate(360deg); }

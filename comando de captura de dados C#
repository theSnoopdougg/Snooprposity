    <script>
jQuery(function($){
    // const bckHeader = document.querySelector('.cid-r3yZJLuMgC'); 

     //checar se carregou
     $('document').ready(function(){
        console.log(' estado ',$("select[name=uf]").val());

        if($("select[name=uf]").val() != ""){
                $ost("provaup.com.br/sistema",
                {estado:$("select[name=uf]").val()},
                    function(valor){
                        $("select[name=municipio]").html(valor);
                    }
                );
        }

     });   

    $("#tel1")
        .mask("(99) 9999-9999?9")
        .focusout(function (event) {  
            var target, phone, element;  
            target = (event.currentTarget) ? event.currentTarget : event.srcElement;  
            phone = target.value.replace(/\D/g, '');
            element = $(target);  
            element.unmask();  
            if(phone.length > 10) {  
                element.mask("(99) 99999-999?9");  
            } else {  
                element.mask("(99) 9999-9999?9");  
            }  
        });
    $('#data_nasc').mask("99/99");    
    $('#form_SPCidade').hide();
    
    //mensagem de erro 
    $('#mensagem').hide(); 
    $("input#cep").change(function(){
      var cep_code = $(this).val();
      if( cep_code.length <= 0 ) return;
      $.get("https://viacep.com.br/ws/"+cep_code+"/json/?callback=?",
         function(result){
            var result = result.replace(/\?|\)|\(|\;/g,' ');
            result = JSONarse(result);
            $("input#cep").val(""+result.cep);
            $("select#estado").val( result.uf );
            $("input#bairro").val(""+result.bairro);
            $("input#endereco").val(""+result.logradouro);
            var estadoSelecionado = result.uf;
            $("input#endereco").focus();

            //modificar o select de estados
            $('#form_uf option[value='+estadoSelecionado+']').attr('selected','selected');
            //modiicar select municipios
            
            $ost("provaup.com.br/sistema/indexhp/link_c/combo_municipio",
              {estado:$("select[name=uf]").val(), dc: result.city },
	  	        function(valor){
                    console.log(result.city);
                        if(result.city == 'Brasília'){
                            $('#form_SPCidade').fadeIn(450);
                        }else{
                            $('#form_SPCidade').fadeOut(450); 
                        }  
		                    $("select[name=municipio]").html(valor);
	            }
	        );

           //verifica se alterou o estado para outro que não seja o SP então oculte o campo cidade                     
            if($("select[name=uf]").val()=='SP')
            {
             $('#conteiner_cidade').show();   // Hide div
            }else{ 
             $('#conteiner_cidade').hide();   // show div
            }
         });
   }); 
   //
   $("#form_uf").change(function() {
        
        if($(this).val() == 'SP'){
            $('#form_SPCidade').fadeIn(450);
        }else{
            $('#form_SPCidade').fadeOut(450); 
        }
        $ost("provaup.com.br/sistema/indexhp/link_c/combo_municipio02",
        {estado:$("select[name=uf]").val()},
            function(valor){
                $("select[name=municipio]").html(valor);
            }
        );

    });

     //email
     $("#form_email").change(function(){
        /* em via via post para pesquisa se existe um email de mesmo nome*/
            $ost("provaup.com.br/sistema/indexhp/link_c/pesquisar_homonimo_email_form",{email:$(this).val(),campanha:163}, function(valor){
                if(valor == "1"){ 
                    alert("Esse E-mail já foi Cadastrado");
                    $("#form_email").css("border-color","red");
                }
            });
     });

     $("#tel1").change(function(){
        
        /* em via via post para pesquisa se existe um email de mesmo nome*/
            $ost("provaup.com.br/sistema/indexhp/link_c/verificarTelefoneRepetido_form",{telefone:$(this).val(),campanha:'163',}, function(valor){
                console.log('o valor',valor)
                if(valor == "1"){ 
                    alert("Telefone Já Cadastrado");
                    $("#tel1").css("border-color","red");
                }
            });
     });
//limpar campos
$('#resertarBotao').click(function(){
    $('#form_uf option[value=0]').attr('selected','selected');
    $("select[name=municipio]").html('<option value="0"> MUNICIPIOS </option>');
    $('#form_SPCidade').hide();
   
});    
 
  $("#data_nasc2").datepicker({
    dateFormat: "dd/mm/yy"
  });

});

</script>
